@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@addTagHelper *, AuthoringTagHelpers
@model PTStore.Models.Subcriber

@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2 style="color:#337ab7">Chi tiết thông tin Subcriber</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Email
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Email)
        </dd>
        <dt>
            Trạng thái
        </dt>
        <dd>
            @Html.DisplayFor(model => model.TrangThai)
        </dd>
    </dl>
</div>
<div class="col-md-offset-1">
    <a class="btn btn-default" style="color:white; background-color:#337ab7" asp-action="Edit" asp-route-id="@Model.Id">Sửa trạng thái</a>
</div>
<div style="margin-top:2%">
    <a asp-action="Index">Quay lại trang chủ</a>
</div>
